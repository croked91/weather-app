{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"react-app",
		"plugin:styled-components-a11y/recommended",
		"plugin:effector/recommended",
		"plugin:effector/scope"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["react", "styled-components-a11y", "effector"],
	"rules": {}
}
